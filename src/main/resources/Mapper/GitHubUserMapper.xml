<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhongyuanbbs.demo.Mapper.GithubUserMapper">
    <insert id="addGithubUser" parameterType="com.zhongyuanbbs.demo.domain.GitHubUser">
        INSERT INTO zk_github_user(id,zk_github_username,zk_github_account_id,zk_github_token,zk_github_bio,create_time,last_edit_time)
        VALUES (#{id},#{zkGithubUsername},#{zkGithubAccountId},#{zkGithubToken},#{zkGithubBio},#{createTime},#{lastEditTime})
    </insert>
    <select id="getGitHubUserByToken" parameterType="String" resultType="com.zhongyuanbbs.demo.domain.GitHubUser">
        SELECT id,zk_github_username,zk_github_account_id,zk_github_token,zk_github_bio,create_time,last_edit_time FROM zk_github_user
        WHERE zk_github_token = #{token}
    </select>
</mapper>